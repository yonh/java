<mapper namespace="com.yonh.dao.IUserDao">
    <select id="findAll" resultType="com.yonh.pojo.User">
        select * from user
    </select>

    <!--
    resultType: 返回值类型
    parameterType: 参数类型
    自定义替换参数：#{XXX}
     -->
    <select id="findByCondition" resultType="com.yonh.pojo.User" parameterType="com.yonh.pojo.User">
        select * from user where id = #{id} and username = #{username}
    </select>

</mapper>